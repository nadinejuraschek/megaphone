{
  "name": "twitter-clone",
  "version": "1.0.0",
  "description": "A Twitter-like full stack node.js application. Built with ...",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd .. && cd client && npm run build",
    "install-client": "cd .. && cd client && npm install",
    "start": "node index.js",
    "client": "cd .. && cd client && npm start",
    "dev": "concurrently -n 'server, client' -c 'green, blue' \"nodemon index.js\" \"npm run client\""
  },
